{
  "name": "@amzn/halo-third-party-sdk-events",
  "version": "1.0.0",
  "description": "Third party SDK for Halo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run lint",
    "compile": "tsc",
    "test": "cross-env TS_NODE_FILES=true mocha -r ts-node/register \"./tst/**/*.spec.ts\"",
    "lint": "eslint \"lib/**/*.{ts,tsx}\" \"tst/**/*.{ts,tsx}\"",
    "clean": "rm -rf dist && rm -rf build",
    "reinstall": "rm -rf ./node_modules && npm install",
    "coverage": "nyc -x tst -e .ts -r html -r text-summary -t coverage/.nyc_output --cache npm test",
    "release": "npm run build"
  },
  "keywords": [
    "Halo",
    "SDK"
  ],
  "author": "Amazon.com",
  "contributors": [
    {
      "name": "Jonathan Young",
      "email": "jnathay@amazon.com"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.18.2",
    "node-forge": "^0.10.0",
    "semver": "^7.3.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.1",
    "@types/node-forge": "^0.8.0",
    "@types/sinon": "^7.0.13",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "@amzn/halo-third-party-sdk-model": "*",
    "chai": "^4.1.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "eslint-plugin-tsdoc": "^0.2.6",
    "express": "^4.16.4",
    "mocha": "^5.0.5",
    "nock": "^9.2.3",
    "nyc": "^14.1.1",
    "sinon": "^7.0.13",
    "supertest": "^3.1.0",
    "ts-node": "^6.0.1",
    "typescript": "^3.5.3"
  },
  "repository": "github:amzn/halo-third-party-sdk-for-nodejs",
  "bugs": "TBA",
  "homepage": "TBA",
  "npm-pretty-much": {
    "ciBuild": "never",
    "runRelease": "always",
    "publishLibCommonJs": "true"
  }
}
